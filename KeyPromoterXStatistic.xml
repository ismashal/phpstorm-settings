<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Configure PHP Include Paths...">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'F12' or 'Ctrl+Shift+E'</ShortCut>
          <Description>Configure PHP Include Paths...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Directory">
        <Item>
          <IdeaActionID>FileChooser.GotoProject</IdeaActionID>
          <ShortCut>'Ctrl+2'</ShortCut>
          <Description>Project Directory</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>